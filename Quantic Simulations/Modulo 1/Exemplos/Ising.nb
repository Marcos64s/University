          RowBox[{"1", ",", "3", ",", "2"}], "}"}]}], "]"}], ".", 
       RowBox[{"ConjugateTranspose", "[", "P", "]"}]}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"1", ",", "2"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"3", ",", "4"}], "}"}]}], "}"}]}], "]"}]}], "]"}]}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"traceout", "[", 
